ow\"\n                    type=\"password\"\n                    name=\"new_password\"\n                    placeholder=\"\"\n                    onChange={(e) => (this.new_password = e.target.value)}\n                    />\n                </Form.Group>\n    \n                <Form.Group controlId=\"formBasicPassword\">\n                    <Form.Label>New Password</Form.Label>\n                    <Form.Control\n                    className=\"shadow\"\n                    type=\"password\"\n                    name=\"password_conf\"\n                    placeholder=\"Password\"\n                    onChange={(e) => (this.password_conf = e.target.value)}\n                    />\n                </Form.Group>\n                <Form.Group controlId=\"formBasicCheckbox\">\n                    <Form.Check\n                    type=\"checkbox\"\n                    label=\" I agree terms and condition\"\n                    style={{ fontSize: \"13px\" }}\n                    />\n                </Form.Group>\n                        <Button\n                            type=\"submit\"\n                            style={{\n                            width: \"100%\",\n                            backgroundColor: \"#EFC81A\",\n                            padding: \"15px\",\n                            height: \"55px\"\n                            }}\n                            onClick={this.handleSubmit}\n                        >\n                            Reset Password\n                        </Button>\n                </Form>\n            </div>\n            </div>\n            <div className=\"right-content\">\n            <div className=\"box-form-right-side\">\n                <Form className=\"my-form-right w-75\" style={{marginTop: \"300px\"}}>\n                <Form.Group controlId=\"formBasicEmail\">\n                    <Form.Label>Create New Password</Form.Label>\n                    <Form.Control\n                    className=\"shadow\"\n            