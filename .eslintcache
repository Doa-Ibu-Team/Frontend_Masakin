tSection = document.querySelectorAll(\n      \".\" + profile.ItemList\n    );\n    // remove class active-type and show-section\n    buttons.forEach((item) => item.classList.remove(profile.ItemTitleActive));\n    contentPartSection.forEach((item) =>\n      item.classList.remove(profile.ItemListActive)\n    );\n    // add class active-type and show-section\n    e.target.classList.add(profile.ItemTitleActive);\n    document.querySelector(`#${ListId}`).classList.add(profile.ItemListActive);\n  };\n\n  getMyRecipe = () => {\n    axios.get(baseUrl + `/user/myRecipe/`, config)\n      .then((res) => {\n        console.log(res.data)\n        this.setState({\n          myrecipe: res.data.data\n        })\n      })\n      .catch((err) => {\n        console.log(err)\n      })\n  }\n\n  likedRecipe = () => {\n    axios.get(baseUrl + '/user/getLike', config)\n      .then((res) => {\n        console.log(res.data)\n        this.setState({\n          likedrecipe: res.data.data\n        })\n      })\n      .catch((err) => {\n        console.log(err)\n      })\n  }\n\n  savedRecipe = async () => {\n    axios.get(baseUrl + '/user/getbookmark', config)\n      .then((res) => {\n        console.log(res.data)\n        this.setState({\n          savedrecipe: res.data.data\n        })\n      })\n      .catch((err) => {\n        console.log(err)\n      })\n  }\n\n  componentDidMount = () => {\n    this.getMyRecipe()\n    this.savedRecipe()\n    this.likedRecipe()\n\n  }\n\n  render() {\n    // console.log(this.state.profile);\n    const { myrecipe, savedrecipe, likedrecipe, } = this.state\n    console.log(this.state)\n    return (\n      <>\n        <div className={profile.Section}>\n          <div\n            className={profile.Image + \" mx-auto\"}\n            style={{ backgroundImage: `url(${ImageProfile})` }}\n          >\n            <img\n              src={EditProfileBtn}\n              className={profile.EditButton}\n              alt=\"\"\n              height=\"24px\"\n              width=\"24px\"\n            //   onClick={this.updateEditSection}\n            />\n          </div>\n          <div className={\"mx-auto text-center \"}>\n            <p className={profile.Username + \" mt-2\"}>\n              {localStorage.getItem('name')}\n            </p>\n          </div>\n          <div\n            className={\n              `${profile.EditSection} mx-auto ${profile.Show}`\n            }\n          >\n            <button className={profile.DefaultBtn + \" d-block\"}>\n              Change Photo Profile\n            </button>\n            <button className={profile.DefaultBtn + \" d-block\"}>\n              Change Password\n            </button>\n          </div>\n        </div>\n\n\n        <Container>\n          <div className=\"my-list d-flex mt-5\">\n            <p className={profile.ItemTitle + \" \" + profile.ItemTitleActive} data-id={\"MySection\"} onClick={this.myListActive}>\n              My Recipe\n            </p>\n            <p className={profile.ItemTitle} data-id={\"SavedSection\"} onClick={this.myListActive}>\n              Saved Recipe\n            </p>\n            <p className={profile.ItemTitle} data-id={\"LikedSection\"} onClick={this.myListActive}>\n              Liked Recipe\n            </p>\n          </div>\n        </Container>\n        <hr />\n        <Container>\n          <div className={profile.ItemList + \" \" + profile.ItemListActive} id=\"MySection\">\n            <div className={profile.CardWrapper}>\n              {myrecipe && myrecipe.map(({ id_recipe, title, img }) => {\n                return (\n                  <>\n                    <Link to={{\n                      pathname: `/recipe/${id_recipe}`\n                    }}>\n                      <div href=\"/recipe\" className={profile.CardList}>\n                        <img src={'http://127.0.0.1:8000/' + img} style={{ width: \"270px\", height: \"180px\" }} />\n                        <h1>{title}</h1>\n                      </div>\n                    </Link>\n                  </>\n                )\n              })\n              }\n            </div>\n          </div>\n\n          <div className={profile.ItemList} id=\"SavedSection\">\n            <div className={profile.CardWrapper}>\n              {savedrecipe && savedrecipe.map(({ id_recipe, title, img }) => {\n                return (\n                  <>\n                    <Link to={{\n                      pathname: `/recipe/${id_recipe}`\n                    }}>\n                      <div href=\"/recipe\" className={profile.CardList}>\n                        <img src={'http://127.0.0.1:8000/' + img} style={{ width: \"270px\", height: \"180px\" }} />\n                        <h1>{title}</h1>\n                      </div>\n                    </Link>\n                  </>\n                );\n              })}\n            </div>\n          </div>\n\n          <div className={profile.ItemList} id=\"LikedSection\">\n            <div className={profile.CardWrapper}>\n              {\n                likedrecipe && likedrecipe.map(({ id_recipe, title, img }) => {\n                  return (\n                    <>\n                      <Link to={{\n                        pathname: `/recipe/${id_recipe}`\n                      }}>\n                        <div href=\"/recipe\" className={profile.CardList}>\n                          <img src={'http://127.0.0.1:8000/' + img} style={{ width: \"270px\", height: \"180px\" }} />\n                          <h1>{title}</h1>\n                        </div>\n                      </Link>\n                    </>\n                  )\n                })\n              }\n            </div>\n          </div>\n\n        </Container>\n      </>\n    );\n  }\n}\nexport default Profile;","/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/searchItems/index.jsx",["151"],"import React, { Component } from \"react\";\nimport { Container } from \"react-bootstrap\";\nimport {Link} from 'react-router-dom'\nimport \"../landingPage/style.css\";\n\nexport default class popularRecipe extends Component {\n  \n  render() {\n    const { id_recipe, title, img } = this.props\n    console.log(this.props)\n    return (\n      <>\n\n        <ul className=\"galery\">\n          <Link to={{\n            pathname: `/recipe/${id_recipe}`\n          }}>\n            <li>\n              <img\n                className=\"feed-food\"\n                src={'http://127.0.0.1:8000' + img}\n                alt=\"\"\n              />\n              <div className=\"text-block\">\n                <h4>\n                  {title}\n                </h4>\n              </div>\n            </li>\n          </Link>\n        </ul>\n      </>\n    );\n  }\n}","/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/assets/index.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/index.js",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/pages/auth/CodeReset.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/pages/recipe/DetailRecipe.jsx",["152"],"import React, { Component } from 'react'\n\nimport Navbar from '../../components/navbar/navbar'\nimport Detail from '../../components/Detail/Detail'\nimport Footer from '../../components/landingPage/footer'\n\n\nclass DetailRecipe extends Component {\n\tconstructor(){\n\t\tsuper()\n\t}\n\trender () {\n\t\tconsole.log(this.props)\n\t\treturn (\n\t\t\t<>\n\t\t\t\t<Navbar />\n\t\t\t\t<Detail id={this.props.match.params.id}/>\n\t\t\t\t<Footer />\n\t\t\t</>\n\t\t)\n\t}\n}\n\nexport default DetailRecipe","/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/pages/home/Home.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/auth/Register.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/auth/ForgotPass.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/addRecipe/addRecipe.jsx",["153","154","155","156"],"import React, { Component } from 'react';\nimport { Jumbotron, Button, Form } from \"react-bootstrap\";\nimport { Redirect } from \"react-router-dom\";\nimport squareImg from \"../../assets/icons/imageupload.png\";\nimport css from \"./addRecipe.module.css\";\nimport axios from 'axios';\nconst config = {\n    headers: {\n        'Content-Type': 'multipart/form-data',\n        \"x-access-token\": \"Bearer \" + localStorage.getItem(\"token\")\n    },\n}\n\nconst baseUrl = process.env.REACT_APP_BASE_URL\nconsole.log(config)\n\nclass addRecipe extends Component {\n\n    constructor(props) {\n        super(props)\n        this.state = {\n            title: '',\n            ingredients: '',\n            videos: [],\n            file: null,\n            img: [],\n            dataInserted: false\n        }\n        this.handleFile = this.handleFile.bind(this)\n        this.handleVideo = this.handleVideo.bind(this)\n    }\n\n    changeHandler = (e) => {\n        this.setState({ [e.target.name]: e.target.value })\n    }\n    handleFile(event) {\n        this.setState({\n            file: URL.createObjectURL(event.target.files[0]),\n            img: event.target.files\n        })\n    }\n\n    handleVideo(event) {\n        this.setState({\n            videos: event.target.files\n        })\n    }\n\n    submitHandler = e => {\n        console.log('aaaa')\n        let formdata = new FormData()\n        formdata.append('user_id', localStorage.getItem('user_id'))\n        formdata.append('title', this.state.title)\n        formdata.append('ingredients', this.state.ingredients)\n        for (let i = 0; i < this.state.img.length; i++) {\n            formdata.append('img', this.state.img[i])\n        }\n        for (let i = 0; i < this.state.videos.length; i++) {\n            formdata.append(\"videos\", this.state.videos[i]);\n        }\n        console.log(formdata)\n        e.preventDefault()\n        axios.post(baseUrl + '/recipe/b/add', formdata, config)\n            .then(response => {\n                console.log(response)\n                this.setState({\n                    dataInserted: true\n                })\n            })\n            .catch(error => {\n                console.log(error)\n            })\n\n    }\n\n    render() {\n        console.log(this.state)\n        const { user_id, title, ingredients, img, videos } = this.state\n        return (\n            <div className='container'>\n                {/* {this.state.dataInserted && <Redirect to=\"/profile\" />} */}\n                <div className=\"card border-white mb4\" style={{ width: \"99%\", height: \"auto\", marginLeft: \"70px\" }}>\n                    <div className=\"card-body\">\n                        <Form encType=\"multipart/form-data\" onSubmit={this.submitHandler}>\n                            <Jumbotron className=\"container-gap\" style={{ backgroundColor: '#F6F5F4' }}>\n                                <p style={{ color: \"gray\", fontSize: \"5px\", position: \"relative\", right: \"1rem\" }} className=\"mb-n1\">Image</p>\n                                <div className=\"col-md-8 row\">\n                                    <img src={squareImg} alt=\"\" />\n                                    <div className={css.UploadBtn}>Add Photo</div>\n                                    <input type=\"file\" name='img' onChange={this.handleFile} autoComplete='off' placeholder=\"\" />\n                                    <img src={this.state.file} />\n                                </div>\n                            </Jumbotron>\n                            <Form.Group controlId=\"formBasicText\" style={{ margin: '2rem auto 2rem auto' }}>\n                                <Form.Control type=\"text\" name='title' value={title} onChange={this.changeHandler} autoComplete='off' placeholder=\"Title\" style={{ background: '#F6F5F4' }} />\n                            </Form.Group>\n                            <Form.Group controlId=\"formBasicText\" style={{ margin: '2rem auto 2rem auto' }}>\n                                <Form.Control as=\"textarea\" rows={10} name='ingredients' value={ingredients} onChange={this.changeHandler} autoComplete='off' placeholder=\"Ingredients\" style={{ background: '#F6F5F4' }} />\n                            </Form.Group>\n                            <Form.Group controlId=\"formBasicText\" style={{ margin: '2rem auto 2rem auto' }}>\n                                <Form.Control type=\"file\" name='videos' onChange={this.handleVideo} autoComplete='off' placeholder=\"Videos\" style={{ background: '#F6F5F4' }} multiple />\n                            </Form.Group>\n                            <div className='d-flex justify-content-center'>\n                                <Button variant=\"warning\" type=\"submit\" style={{ width: '300px' }}>\n                                    Submit\n                                </Button>\n                            </div>\n                        </Form>\n                    </div>\n                </div >\n            </div >\n\n        );\n    }\n}\n\nexport default addRecipe;","/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/Detail/Detail.jsx",["157","158","159"],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/landingPage/newRecipe.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/redux/reducers/Auth.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/landingPage/NewRecipe/index.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/landingPage/Popular/index.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/landingPage/Footer/index.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/landingPage/Navbar/index.jsx",[],"/home/dev7/Project/Kuis Mingguan/TimProjek/Frontend_Masakin/src/components/landingPage/footer.jsx",[],"/home/dev7/Pr